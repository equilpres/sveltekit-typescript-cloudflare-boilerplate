{
	"name": "sveltekit-typescript-cloudflare-boilerplate",
	"description": "Minimalistic boilerplate for SvelteKit projects in TypeScript, optimized for Cloudflare Pages",
	"version": "0.0.0",
	"author": {
		"name": "Слава Соколов",
		"username": "equilpres",
		"url": "https://github.com/equilpres"
	},
	"type": "module",
	"engines": {
		"node": "^18.18"
	},
	"license": "MIT",
	"scripts": {
		"dev": "vite dev --host ::",
		"build": "pnpm sync && pnpm lint && vite build",
		"preview": "wrangler pages dev ./.svelte-kit/cloudflare",
		"sync": "svelte-kit sync",
		"svelte-check": "svelte-check",
		"lint": "svelte-check && prettier --check --cache . && eslint .",
		"fmt": "prettier --write --cache .",
		"bump": "pnpm update --latest"
	},
	"dependencies": {},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20240512.0",
		"@sveltejs/adapter-cloudflare": "^4.4.0",
		"@sveltejs/kit": "^2.5.10",
		"@sveltejs/vite-plugin-svelte": "^3.0.2",
		"@typescript-eslint/eslint-plugin": "^7.5.0",
		"@typescript-eslint/parser": "^7.5.0",
		"autoprefixer": "^10.4.19",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"postcss": "^8.4.38",
		"postcss-load-config": "^5.0.3",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.2",
		"prettier-plugin-tailwindcss": "^0.5.13",
		"svelte": "^4.2.12",
		"svelte-check": "^3.6.9",
		"tailwindcss": "^3.4.3",
		"tslib": "^2.6.2",
		"typescript": "^5.4.3",
		"vite": "^5.2.7",
		"wrangler": "^3.57.1"
	}
}
